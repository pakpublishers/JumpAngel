<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="AngelDetailLable">
        <item  name="android:textColor">@android:color/black</item>
        <item  name="android:textStyle">normal</item>
        <item  name="android:textSize">12dip</item>
    </style>
   
    <style name="MenuItemButton">
        <item  name="android:textColor">@android:color/white</item>
        <item  name="android:gravity">left|center_vertical</item>
        <item  name="android:textSize">13dip</item>
        <item  name="android:textStyle">bold</item>
        <item  name="android:paddingLeft">15dip</item>
    </style>
    
    <style name="MyButtonstyle">
        <item  name="android:background">@drawable/graybtn</item>
        <item  name="android:textColor">@android:color/white</item>
        
    </style>
    <style name="MyButtonstyleselected">
        <item  name="android:background">@drawable/mybutton</item>
        <item  name="android:textColor">@android:color/white</item>
        
    </style>
    
    <style name="myRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating</item>
        <item name="android:minHeight">20dip</item>
        <item name="android:maxHeight">20dip</item>
    </style>
     <style name="MyTheme" parent="@android:style/Theme.Holo.Light"> 
	  <item name="android:windowBackground">@color/white</item>
	  <item name="android:windowNoTitle">true</item>
	  <item name="android:textColor">@color/black</item>
	  
	  
	 </style>
    
    
    

</resources>